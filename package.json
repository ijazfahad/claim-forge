{
  "name": "claim-validator",
  "version": "1.0.0",
  "description": "AI-powered medical claim validation backend with PDM and SSP agents",
  "main": "dist/app.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/app.js",
    "dev": "nodemon src/app.ts",
    "test": "ts-node src/test/test-runner.ts",
    "test:all": "ts-node src/test/test-runner.ts",
    "test:sanity": "ts-node src/test/test-runner.ts sanity",
    "test:planner": "ts-node src/test/test-runner.ts planner",
    "test:google-search-agent": "ts-node src/test/test-runner.ts google-search-agent",
    "test:firecrawl-agent": "ts-node src/test/test-runner.ts firecrawl-agent",
    "test:research-agent": "ts-node src/test/test-runner.ts research-agent",
    "test:evaluator-agent": "ts-node src/test/test-runner.ts evaluator-agent",
    "test:evaluate": "ts-node src/test/test-runner.ts evaluate",
    "test:integration": "ts-node src/test/test-runner.ts integration",
    "test:google": "ts-node src/test/test-runner.ts google",
    "test:firecrawl": "ts-node src/test/test-runner.ts firecrawl",
    "test:workflow": "ts-node src/test/test-workflow.ts",
    "test:steps": "ts-node src/test/validation-steps-test.ts",
    "test:services": "ts-node src/test/services-test.ts",
    "test:cms": "ts-node src/test/cms-ncci-test.ts",
    "test:cms:comprehensive": "ts-node src/test/comprehensive-cms-test.ts",
    "test:web-interface": "ts-node src/test/web-interface-test.ts",
    "test:comprehensive-workflow": "ts-node src/test/comprehensive-workflow.test.ts",
    "docker:build": "docker build -t claim-validator .",
    "docker:run": "docker run -p 3000:3000 --env-file .env claim-validator",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up --build",
    "docker:prod": "docker-compose up --build -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:clean": "docker-compose down -v --rmi all",
    "migrate": "npx ts-node src/migrations/migrate.ts",
    "migrate:status": "npx ts-node src/migrations/migrate.ts status",
    "update:cms": "npx ts-node -e \"import { buildLatest } from './src/services/cms-ncci-validator'; buildLatest({ verbose: true }).then(() => console.log('âœ… CMS/NCCI database updated successfully')).catch(console.error)\""
  },
  "dependencies": {
    "@openai/agents": "^0.1.6",
    "@types/pg": "^8.15.5",
    "axios": "^1.12.2",
    "cheerio": "^1.1.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.0",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.0",
    "joi": "^17.11.0",
    "morgan": "^1.10.0",
    "openai": "^4.104.0",
    "pg": "^8.16.3",
    "unzipper": "^0.12.3",
    "xlsx": "^0.18.5",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.0",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.19.17",
    "@types/unzipper": "^0.10.11",
    "jest": "^29.7.0",
    "nodemon": "^3.0.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "medical",
    "claims",
    "validation",
    "ai",
    "healthcare",
    "rcm"
  ],
  "author": "Fahad Ijaz",
  "license": "MIT"
}
